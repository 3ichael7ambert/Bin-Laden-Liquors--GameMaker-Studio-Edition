        <arguments>
          <argument>
            <kind>1</kind>
            <string>// Move the hand to wherever the cursor moves
self.x = mouse_x - 75;
self.y = mouse_y - 15;

// Check if game over and move to that screen if it is.
// Game ends if 10 terrorists are missed or 5 hostages are killed
if (self.Misses &gt;= 10 or self.Hostages &gt;= 5)
{
    // Calculate current score
    global.CurrentScore = ((self.Kills*100)-(self.Hostages*1000));
    
    //If current score is higher than high score set the new High score!
    if (global.CurrentScore &gt; global.HighScore)
    {
        global.newHighScore = 1;
        global.HighScore = global.CurrentScore;
        ini_open("gamesave.ini");
        ini_write_real("player_section", "player", global.HighScore);
        ini_close();
        show_debug_message("Saved new score " + string(global.HighScore));
    }
    //End Game!
    room_goto(rm_game_over);
}

// Increase difficulty every 5 kills
if (self.Kills &gt;= self.threshhold)
{
    show_debug_message("Should happen now");
    if (self.IncreaseDifficulty == 0)
    {
        if (self.SpawnSpeed &gt; .5)
        {
            self.SpawnSpeed -= .5;
        }
        if (self.StandSpeed &gt; 1)
        {
            self.StandSpeed -= .75;
        }
        if (self.RiseSpeed &gt; .5)
        {
            self.RiseSpeed -= .5;
        }
        self.IncreaseDifficulty += 1;
        self.threshhold += 5;
        show_debug_message("Increasing Difficulty");
    }
}
else
{
    self.IncreaseDifficulty = 0;
}
</string>
