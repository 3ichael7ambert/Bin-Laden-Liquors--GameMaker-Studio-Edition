            <string>if (self.Ammo &gt;= 1 and self.CanFire == 1)
{
//If we have ammo we will fire a shot
    if (self.Ammo == 1)
    {
        //However if we're on our last bullet we need to tell the player to reload
        if (!instance_exists(obj_game_clip))
        {
            instance_create(575, 250, obj_game_clip);
            self.CanFire = 0;
        }
    }
    alarm[0]=5;
}
else if(self.Ammo &lt;= 0)
{
//If they are out of bullets and for some reason haven't been told, tell them
    if (!instance_exists(obj_game_clip))
    {
        instance_create(575, 250, obj_game_clip);
        self.CanFire = 0;
    }
}

</string>
