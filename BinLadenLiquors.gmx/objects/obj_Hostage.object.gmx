        <arguments>
          <argument>
            <kind>1</kind>
            <string>//intx will represent how many seconds it will take to stand up
var intx = (100/((Player).RiseSpeed*60));

if (self.Standing == 1)
{
    //Determines what to do if he is in the process of standing up
    // Rise from 292 to 192
    self.y -= intx;
    if (self.y &lt;= 170)
    {
        // Once the peak is reached we stop standing and wait before ducking
        self.Standing = 0;
        self.Idling = 1;   
    }
}

if (self.Idling == 1)
{
    //Triggers the idle timer before ducking back down
    alarm[3] = (Player).StandSpeed*60;
    self.Idling = 0;
}

if (self.Falling == 1)
{
    // Returns to hiding. Player will receive a miss if not shot 
    // Fall from 292 to 192
    self.y += intx;
    if (self.y &gt;= 290)
    {
        //Stop falling. At this point the shot/missed alarm goes off
        //Since the Shot variable is still 0 the game assumes the player
        //did not shoot the terrorist.
        alarm[0] = 5;
        self.Falling = 0;
    }
}
</string>
