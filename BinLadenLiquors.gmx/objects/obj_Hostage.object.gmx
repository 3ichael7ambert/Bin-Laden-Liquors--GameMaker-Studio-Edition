        <arguments>
          <argument>
            <kind>1</kind>
            <string>//Hostage Timer C0dez

self.visible = false;

if (self.Shot == 1)
{
    //If the terrorist is shot add 1 to the kill count
    //Set the shot variable to 2 (to avoid issues of repeating)
    //Spawn a bloodsplat for the user to see.
    with (Player) Hostages += 1;
    show_debug_message("Hostages: " + string((Player).Hostages));
    self.Shot = 2;
    with (Desk) HostageSet = 0;
    instance_create(self.BloodX, self.BloodY, obj_game_bloodSplat);
}
else if (self.Shot == 0)
{
    with (Desk) HostageSet = 0;
    instance_destroy();
}

