  <code>// create global variables for our score
var global.HighScore = 0;
var global.CurrentScore = 0;

// If the game has been played before load highscore else their high is 0
if (file_exists("gamesave.ini"))
{
    ini_open("gamesave.ini");
    global.HighScore = ini_read_real("player_section", "player", 0);
    ini_close();
    show_debug_message("Loaded");
    show_debug_message(global.HighScore);
}
else
{
    ini_open("gamesave.ini");
    ini_write_real("player_section", "player", global.HighScore);
    ini_close();
    show_debug_message("Created Save");
}</code>
